{
  "name": "vovan-ve/array-dumper",
  "description": "Dumper for constant arrays",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vovan-VE",
      "email": "vovan-ve@yandex.ru"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "lib-pcre": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --verbose"
  },
  "archive": {
    "exclude": [
      "/tests/",
      "/phpunit.xml.dist"
    ]
  },
  "autoload": {
    "psr-4": {
      "VovanVE\\array_dumper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VovanVE\\array_dumper\\tests\\": "tests/"
    }
  }
}
